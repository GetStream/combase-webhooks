{
	"name": "worker",
	"version": "0.1.0",
	"description": "Combase Webhook Worker Module",
	"main": "src/main.js",
	"module": "src/main.js",
	"author": "Stream",
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": "15.x",
		"yarn": "1.x"
	},
	"private": true,
	"dependencies": {
		"@captain-hook/core": "^0.3.7",
		"@captain-hook/rascal-engine": "^0.1.8",
		"deepmerge": "^4.2.2",
		"fs-extra": "^9.1.0",
		"pm2": "^4.5.1",
		"slash": "^3.0.0",
		"utils": "*",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"clean": "rm -rf .data",
		"docker:build": "docker-compose build",
		"docker:run": "docker-compose up -d --scale app=10",
		"prep": "node --es-module-specifier-resolution=node ./scripts/prepare",
		"prestart": "yarn prep",
		"start": "pm2-runtime start pm2.prod.js",
		"start:dev": "pm2-runtime start pm2.dev.js",
		"dev": "node --trace-warnings --es-module-specifier-resolution=node ./src/main.js"
	}
}
